{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ng-build": "ng build --prod --source-map --extra-webpack-config webpack.extra.js",
    "build": "node ./build/build-pre.js && rm -rf ../docs && rm -rf dist && npm run ng-build && mv dist/ui ../docs",
    "deploy": "git checkout deploy && git rebase main && npm run build && echo lttemp.com>../docs/CNAME && git add -A && git commit --amend --no-edit && git push --force && git checkout main",
    "analyze": "npm run ng-build && npx source-map-explorer dist/ui/**/*.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/cdk": "~11.2.10",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/material": "~11.2.10",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "chart.js": "^2.9.4",
    "ng2-charts": "^2.4.2",
    "ngx-build-plus": "^11.0.0",
    "ngx-timeago": "^2.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular-eslint/builder": "2.1.0",
    "@angular-eslint/eslint-plugin": "2.1.0",
    "@angular-eslint/eslint-plugin-template": "2.1.0",
    "@angular-eslint/schematics": "2.1.0",
    "@angular-eslint/template-parser": "2.1.0",
    "@angular/cli": "~11.2.7",
    "@angular/compiler-cli": "~11.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  }
}
